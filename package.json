{
  "name": "fe-react-ark",
  "version": "0.0.1",
  "description": "The best react boilerplate arch for made your projects, really easy to develop.",
  "main": "index.jsx",
  "repository": "https://github.com/barthachijuu/FeReactArk.git",
  "author": "Bartolo Amico <bartolo.amico@gmail.com>",
  "contributors": [
    "Gabriele Bonumore <g.bonumore@emergento.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 1.1.x",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "analyze": "better-npm-run analyze",
    "archive": "better-npm-run archive",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "generate": "node node_modules/plop/src/plop.js --plopfile generators/index.js",
    "get-version": "node internals/get-version",
    "lint": "eslint --no-ignore --ext .js --ext .jsx config internals server web",
    "lint:fix": "yarn lint --fix",
    "lint:web": "eslint --ext .jsx --ext .js --no-ignore web",
    "prettify": "prettier --write",
    "reinit": "node internals/clean",
    "setup": "npm i -D chalk fs-extra readline semver yargs && node internals/setup",
    "start": "better-npm-run start",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "lint-staged": {
    "*{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "betterScripts": {
    "analyze": {
      "command": "node internals/analyze",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "archive": {
      "command": "node internals/archive",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node internals/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "yarn lint && yarn test && yarn clean && yarn compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "yarn deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "yarn deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node internals/dev-server --colors --verbose",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged && scss-lint && yarn run lint:web"
    }
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^2.0.1",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^2.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "connected-react-router": "^6.3.2",
    "copy-webpack-plugin": "^5.0.2",
    "dev": "^0.1.3",
    "dot-prop-immutable": "^1.5.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "jss-preset-default": "^4.5.0",
    "loglevel": "^1.6.1",
    "normalizr": "^3.3.0",
    "open-color": "^1.6.3",
    "postcss-font-magician": "^2.2.1",
    "postcss-icon": "^8.0.1",
    "postcss-icon.material-design": "^0.0.3",
    "purifycss-webpack": "^0.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-jss": "^8.6.1",
    "react-redux": "^6.0.1",
    "react-redux-form": "^1.16.12",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-saga-test-plan": "^3.7.0",
    "reselect": "^4.0.0",
    "shelljs": "^0.8.3",
    "simple-progress-webpack-plugin": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "webpack-monitor": "^1.0.14",
    "webpack-visualizer-plugin": "^0.1.11"
  },
